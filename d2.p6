use v6;

my $inputs = q:to 'EOS';





EOS


my Array @input;

for split("\n", $inputs, :skip-empty) -> $line {
    my @movements = split("", chomp($line), :skip-empty);
    push @input, @movements;
}

my int @grid1[3;3] = ( [1..3], 
                       [4..6], 
                       [7..9]);

my @grid2[;] = ( [1], 
                 [2,3,4], 
                 [5,6,7,8,9], 
                 [ < A B C >],
                 [ < D >],
               );

sub get_code(int :$x is copy, int :$y is copy, :@grid) {

    my @code;
    for @input -> @key {
        for @key -> $movement {
            given $movement {
                when 'U'
                    { $y-- if $y > 0; }
                when 'D' 
                    { $y++ if $y < 2; }
                when 'R' 
                    { $x++ if $x < 2; }
                when 'L' 
                    { $x-- if $x > 0; }
                default
                    { say "invalid movement $movement!"; }
            }
        }
        push @code, @grid[$y;$x];
    }

    return @code;
}

say get_code( x => 1,  y => 1, grid => @grid1);
