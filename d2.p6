use v6;

my $inputs = q:to 'EOS';





EOS


my Array @input;

for split("\n", $inputs, :skip-empty) -> $line {
    my @movements = split("", chomp($line), :skip-empty);
    push @input, @movements;
}

my @grid1[;] = ( [1..3],
                 [4..6],
                 [7..9]);

my @grid2[;] = ( [Nil,Nil,1,Nil,Nil],
                 [Nil,2,3,4,Nil],
                 [5,6,7,8,9],
                 [ Nil, 'A', 'B', 'C', Nil],
                 [ Nil, Nil, 'D', Nil, Nil],
               );

sub get_code(int :$x is copy, int :$y is copy, :@grid) {

    my @code;
    for @input -> @key {
        for @key -> $movement {
            given $movement {
                when 'U'
                    { $y-- if @grid[$y-1;$x]; }
                when 'D'
                    { $y++ if @grid[$y+1;$x]; }
                when 'R'
                    { $x++ if @grid[$y;$x+1]; }
                when 'L'
                    { $x-- if @grid[$y;$x-1]; }
                default
                    { say "invalid movement $movement!"; }
            }
        }
        push @code, @grid[$y;$x];
    }

    return @code;
}

say get_code( x => 1,  y => 1, grid => @grid1).join;
say get_code( x => 0,  y => 2, grid => @grid2).join;
